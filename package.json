{
  "name": "netlify-wp-login-honeypot",
  "version": "0.0.0",
  "type": "module",
  "bin": {
    "wphp": "./bin/wphp.js"
  },
  "scripts": {
    "clean": "npm-run-all --parallel clean:*",
    "clean:cli": "rimraf ./bin",
    "clean:vite": "rimraf ./build",
    "dev": "npm-run-all --parallel dev:*",
    "dev:cli": "npm run build:cli -- --watch",
    "dev:vite": "vite dev",
    "build": "npm run clean && npm-run-all --parallel build:*",
    "build:cli": "tsup src/cli/*.ts --out-dir bin --no-splitting --cjsInterop --shims --tsconfig tsconfig.node.json",
    "build:vite": "vite build",
    "prepack": "npm run build:cli",
    "prepare": "svelte-kit sync",
    "preview": "vite preview",
    "test": "playwright test",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
  },
  "dependencies": {
  },
  "devDependencies": {
    "@antora/expand-path-helper": "^2.0.0",
    "@sveltejs/adapter-auto": "^2.1.0",
    "@sveltejs/kit": "^1.22.6",
    "@types/inquirer": "^9.0.3",
    "@types/node": "18",
    "@types/shelljs": "^0.8.12",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "commander": "^11.0.0",
    "degit": "^2.8.4",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-svelte": "^2.32.4",
    "inquirer": "^9.2.10",
    "log-symbols": "^5.1.0",
    "npm-run-all2": "^6.0.6",
    "pkg-dir": "^7.0.0",
    "rimraf": "^5.0.1",
    "shelljs": "^0.8.5",
    "svelte": "^4.2.0",
    "svelte-check": "^3.5.0",
    "tslib": "^2.6.1",
    "tsup": "^7.2.0",
    "typescript": "^5.1.6",
    "vite": "^4.4.9"
  },
  "lint-staged": {
    "*.(ts|js)": [
      "eslint --cache --fix"
    ]
  }
}
